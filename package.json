{
  "name": "phaser-typescript-webpack-boilerplate",
  "version": "1.0.0",
  "description": "Starter boilerplate for an Phaser game",
  "main": "dist/index.js",
  "scripts": {
    "start": "webpack-dev-server -d --inline --hot --progress --no-info --port 9000",
    "build": "rimraf ./dist && webpack -p",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "author": "Yuval Saraf (unimonkiez@gmail.com)",
  "license": "MIT",
  "devDependencies": {
    "@types/enzyme": "^2.8.8",
    "@types/jest": "^21.1.0",
    "@types/node": "^8.0.29",
    "@types/react": "^16.0.5",
    "@types/react-dom": "^15.5.4",
    "@types/react-redux": "^5.0.9",
    "@types/react-test-renderer": "^15.5.4",
    "@types/redux": "^3.6.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "css-loader": "^0.28.7",
    "enzyme": "^2.9.1",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "inject-loader": "^3.0.1",
    "jest": "^21.1.0",
    "js-css-loader": "0.0.3",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-redux": "^5.0.6",
    "react-test-renderer": "^15.6.1",
    "redux": "^3.7.2",
    "rimraf": "^2.6.2",
    "style-loader": "^0.18.2",
    "svg-react-loader": "^0.4.5",
    "ts-jest": "^21.0.1",
    "ts-loader": "^2.3.7",
    "typescript": "^2.5.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.8.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  }
}
